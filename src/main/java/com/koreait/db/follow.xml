<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="follow">
	<select id="followingList" parameterType="string" resultType="string">
		select f_follow from tb_following where f_memidx=#{f_memidx}
	</select>
	
	<select id="followingInfo" parameterType="string" resultType="map">
		select * from tb_member where m_idx = #{m_idx}
	</select>
	
	
</mapper>